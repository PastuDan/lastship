{
  "name": "lastship",
  "productName": "LastShip App / OneShip App",
  "main": "electron/main.js",
  "version": "0.1.0",
  "private": true,
  "homepage": "./",
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "electron": "^11.1.0",
    "nightmare": "^3.0.2",
    "nodemon": "^2.0.6",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-scripts": "4.0.1",
    "web-vitals": "^0.2.4"
  },
  "scripts": {
    "start": "cross-env BROWSER=none react-scripts start",
    "electron": "cross-env ELECTRON_START_URL=http://localhost:3000 && nodemon --watch ./electron --exec \"electron .\"",
    "dev": "cross-env ELECTRON_START_URL=http://localhost:3000 &&, nodemon --watch . --exec \"electron . --debug\"",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "printWidth": 120,
    "arrowParens": "avoid"
  },
  "nodemonConfig": {
    "delay": "2500",
    "watch": [
      "electron/"
    ],
    "env": {
      "__DEV__": "true"
    }
  },
  "devDependencies": {
    "cross-env": "^7.0.3"
  }
}
